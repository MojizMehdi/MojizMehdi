<?xml version="1.0" encoding="utf-8" ?>
<ElementFactory>
  <Module name="Login">
    <Element keyword="Login_UserId" locator="//input[@name='Username']"/>
    <Element keyword="Login_Password" locator="//input[@name='Password']"/>
    <Element keyword="Login_SignIn_Button" locator="//button[@class='btn submit-btn ng-binding']"/>
    <Element keyword="Login_OTP_field" locator="//input[@name='OneTimePassword']"/>
    <Element keyword="Login_OTP_Verify_Button" locator="//button[@class='btn submit-btn']"/>
    
  </Module>
    
  <Module name ="BeneficiaryManagement">
     <Element keyword="BeneficiaryManagement_Link" locator="//*[contains(text(),'Beneficiary Management ')]"/>
    <Element keyword="BeneficiaryManagement_AddNewBtn" locator="//a[@title = 'Add New' and @class='btn add btn-responsive-fix d-md-block d-lg-block d-xl-block ng-scope']"/>
    <Element keyword="BeneficiaryManagement_Bank" locator="//select[@name='bank']"/>
    <Element keyword="BeneficiaryManagement_AccountNo" locator="//input[@name='account']"/>
    <Element keyword="BeneficiaryManagement_BeneNick" locator="//input[@name='beneNick']"/>
    <Element keyword="BeneficiaryManagement_PayeeEmail" locator="//input[@name='PayeeEmail']"/>
    <Element keyword="BeneficiaryManagement_PayeeMobileNumber" locator="//input[@name='PayeeMobileNumber']"/>
    <Element keyword="BeneficiaryManagement_Validate_Button" locator="//button[@class='btn submit-btn ng-binding']"/>
  </Module>

  <Module name ="Pay">
    <Element keyword="Pay_Link" locator="//*[contains(text(),'Pay ')]"/>
    <Element keyword="Pay_AddNewBtn" locator="//a[contains(text(),'Add New') and @class='btn add float-right']"/>
    <Element keyword="Pay_BillPaymentCategory" locator="//li[contains(text(),'{BillPaymentCategory}')]"/>
    <Element keyword="Pay_BillPaymentCategory_Company" locator="//h2[@class='head ng-binding ng-scope' and contains(text(),'{Pay_BillPaymentCategory_Company}')]"/>
    <Element keyword="Pay_BillPayment_ConsumerNo" locator="//input[@id='ConsumerNo']"/>
    <Element keyword="Pay_BillPayment_NextBtn" locator="//button[@class='btn submit-btn' and text()='Next']"/>
    <Element keyword="Pay_BillPayment_Inquiry_NextBtn" locator="//button[@class='btn btn submit-btn' and text()='Next']"/>
    <Element keyword="Pay_BillPayment_PayBtn" locator="//button[@class='btn submit-btn' and text()='Pay']"/>
    <Element keyword="Pay_Transaction_Success" locator="//div[@class='congrats-div ng-scope']//div[@class='display-table']//p"/>
    <Element keyword="Pay_Transaction_Success_Amount" locator="//h2[@class='ng-binding' and text()='Transaction Amount']//following-sibling::p"/>
    <Element keyword="Pay_Transaction_Success_FromAccount" locator="//h2[@class='ng-binding' and text()='From']//following-sibling::p"/>
    <Element keyword="Pay_Transaction_Success_CompanyName" locator="//h2[@class='ng-binding' and text()='Billing Company Name']//following-sibling::p"/>
    <Element keyword="Pay_Transaction_Success_ConsumerNo" locator="//h2[@class='ng-binding' and text()='Consumer Number']//following-sibling::p"/>
    <Element keyword="Pay_Transaction_ID" locator="//*[contains(text(),'Transaction ID')]//following-sibling::p"/>
    <Element keyword="Pay_Transaction_Scroll_AmountLabel" locator="//label[@for='Amount']"/>
    <Element keyword="Pay_Transaction_ToggleAutoPay" locator="//label[@class='auto-pay']"/>
    <Element keyword="Pay_Transaction_PayBillAmount_RadioBtn" locator="//label[@class='custom-control-label' and contains(text(),'Pay Bill Amount')]"/>
    <Element keyword="Pay_Transaction_PayBillAmount_NextBtn" locator="//div[@class='ng-scope']//button[@class='btn submit-btn' and contains(text(),'Next')]"/>
    <Element keyword="Pay_Transaction_PayBillAmount_AgreeBtn" locator="//button[@class='btn ng-scope' and contains(text(),'Agree')]"/>
    <Element keyword="Pay_Transaction_PayBillAmount_CloseBtn" locator="//div[@class='ng-scope']//a[contains(text(),'Close')]"/>
    <Element keyword="Pay_Transaction_PayBill_BeneSearchTextbox" locator="//input[@type='search']/following-sibling::input"/>
    <Element keyword="Pay_Transaction_PayBill_BeneSearchConsumerNo" locator="//p[@class='text-right ng-binding']"/>
    <Element keyword="Pay_Transaction_PayBill_RatingCloseBtn" locator="//div[@class='text-right ng-scope']//button[contains(text(),'Close')]"/>
    <Element keyword="Pay_Transaction_PayBill_RatingOkBtn" locator="//div[@class='text-right ng-scope']//button[contains(text(),'Ok')]"/>
    <Element keyword="Pay_Transaction_PayBill_TransactionPassword" locator="//input[@name='TransactionPassword']"/>
    <Element keyword="Pay_Transaction_PayBill_Rating" locator="//ul[@class='rating']//li[last()]"/>
  </Module>

  <Module name ="Accounts">
    <Element keyword="Accounts_Link" locator="//a[contains(text(),'Accounts')]"/>
    <Element keyword="Accounts_FromDate" locator="//input[@name='fromDate']"/>
    <Element keyword="Accounts_ToDate" locator="//input[@name='ToDate']"/>
    <Element keyword="Accounts_NoOfDays" locator="//input[@name='NumberOfDays']"/>
    <Element keyword="Accounts_Generate_Button" locator="//button[@type='button' and text()='Generate']"/>
    <Element keyword="Accounts_CSV_Button" locator="//button[@type='button' and text()='CSV']"/>
    <Element keyword="Accounts_PDF_Button" locator="//button[@type='button' and text()='PDF']"/>
  </Module>

  <Module name="Settings">
    <Element keyword="Settings_icon" locator="//div[@class='actions col-lg-5 col-xl-5']//a[contains(text(),'Settings')]"/>
    <Element keyword="Settings_login_old_pass" locator="//input[@ng-model='ChangePassword.oldPassword']"/>
    <Element keyword="Settings_login_new_pass" locator="//input[@ng-model='ChangePassword.newPassword']"/>
    <Element keyword="Settings_login_confirm_pass" locator="//input[@ng-model='ChangePassword.confrimpassword']"/>
    <Element keyword="Settings_change_login_button" locator="//div[@ng-form='LoginPasswordForm']//button[@class='btn submit-btn']"/>
    
  </Module>
</ElementFactory>